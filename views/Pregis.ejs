<%- include('partials/head.ejs') %>
    <%- include('partials/cab.ejs') %>
    <div class="tab-content" id="nav-tabContent">
        <div class="tab-pane fade show active" id="nav-register" role="tabpanel" aria-labelledby="nav-register-tab" tabindex="0">...</div>
    </div>
    <div class="flex flex-col justify-center items-center m-0-auto mt-[12%]">
        <div class="flex flex-col">
            <h1>Registro</h1>
            <form  action="/users/regisPOST" method="POST">
                <div class="mb-3">
                    <label for="username" class="form-label">Username</label>
                    <input type="text" class="form-control" id="username" aria-describedby="userHelp" name="username">
                    <div id="username" class="form-text">Escribe tu nombre de usuario.</div>
                </div>
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Email address</label>
                    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="email">
                    <div id="emailHelp" class="form-text">Escribe tu direccion de correo.</div>
                </div>
                <div class="mb-3">
                    <label for="exampleInputPassword1" class="form-label">Password</label>
                    <input type="password" class="form-control" id="exampleInputPassword1" name="password" maxlength="10" minlength="3">
                </div>
                <button type="submit" class="btn btn-primary text-black">Submit</button>
            </form>
        </div>
    </div>

    <%- include('partials/piepag.ejs') %>
<%- include('partials/foot.ejs') %>